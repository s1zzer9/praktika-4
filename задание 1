    cout << "40 вопрос:" << endl;//40
    cout << endl;
    cout << "40: какого цвета на мне толстовка? " << endl;
    cout << "[1]" << "синия " << endl;
    cout << "[2]" << "красная" << endl;
    cout << "[3]" << "фиолетовая" << endl;
    cout << "Введите ответ: ";
    cin >> a40;
    if (a40 == 3)
    {
        cout << "ответ правильный";

    }
    else if (a40 != 3)
    {
        cout << "ответ не правельный";

    }

    return 0;
}
